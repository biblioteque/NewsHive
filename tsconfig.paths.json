{
	"compilerOptions": {
		"baseUrl": "./src",
		"paths": {
			"@/public": [
				"public/*"
			],
			"@/common": [
				"src/components/common"
			],
			"@/icons": [
				"src/components/icons"
			],
			"@/hooks": [
				"../drafts/hooks"
			],
			"@/components/*": [
				"src/components/*"
			],
			"@/utils/*": [
				"src/utils/*"
			],
			"@/lib/*": [
				"src/lib/*"
			],
			"@/graphql/*": [
				"src/graphql/*"
			],
			"@/screens/*": [
				"../drafts/screens/*"
			],
			"@/store/*": [
				"src/store/*"
			],
			"@/auth": [
				"src/auth"
			],
			"@/styles": [
				"src/styles"
			]
		}
	}
}